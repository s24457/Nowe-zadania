#include <iostream>
# include "vector"

using namespace std;

class Punkt{
private: int x;
private:int y;
public:
    void change (int a, int b){
        x = a;
        y = b;
    }

public: vector<int> get_XY(Punkt ob){
         vector<int> a;
         a.push_back(ob.x);
         a.push_back(ob.y);
        return a;
    }

};

auto main() -> int{
 Punkt one, two;
 vector<int> vec;
 one.change(1,4);
 two.change(5,4);

 vec = one.get_XY(one);
 cout << vec[0] << " " << vec[1] << endl;

 return 0;
}
