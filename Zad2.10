#include <iostream>
# include "vector"

using namespace std;

void print(char a, char b){
    for (int i = (int)a ; i <= (int)b ; ++i) {
        cout << (char)i;
    }
    cout << endl;
}


auto main() -> int{
    vector<char> vec;
    char aa;
    int i = 0;
    do{
       cin >>aa;
        if (aa == 'S'){break;}
        else{
            vec.push_back(aa);
            i++;
        }
    } while (true );

    for (int j = 0; j < vec.size()-1; ++j) {
        print(vec[j], vec[j+1]);
    }
    return 0;
}
